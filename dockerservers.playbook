---

  
- hosts: dockerservers
  become: yes
  vars:
    - firewall_allowed_tcp_ports:
      - "22"
    - sysctl_overwrite:
      - net.ipv4.ip_forward: 1
      - net.ipv6.conf.lo.disable_ipv6: 0
    - docker_users:
      - ansible
  roles:
    - dev-sec.os-hardening
    - geerlingguy.docker
    - geerlingguy.firewall
